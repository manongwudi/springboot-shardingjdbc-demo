<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wudimanong.shardingjdbc.dao.OrderDao">
    <!--批量Insert方法-->
    <insert id="insert" parameterType="com.wudimanong.shardingjdbc.dao.model.OrderPO">
        insert into
        t_order(order_id,trade_type,amount,currency,status,channel,trade_no,user_id,update_time,create_time,remark)
        values (#{orderId}, #{tradeType},#{amount,jdbcType=INTEGER},#{currency},#{status},#{channel},#{tradeNo},#{userId},#{updateTime},#{createTime},#{remark})
    </insert>
</mapper>